"^xterm$"
@Priority=-1
None,	Up,	Shift_R|Page_Up
None,	Down,	Shift_R|Page_Down

"^Mutt.*"
@Priority=1
None,	Up,	Page_Up
None,	Down,	Page_Down
,	Up,	k
,	Down,	j

"^gv"
None,	Up,	Shift_L|space
None,	Down,	space
Shift_L,	Up,	Page_Up
Shift_L,	Down,	Page_Down

"^less "
None,	Up,	Page_Up
None,	Down,	Page_Down

"^irssi "
None,	Up,	Page_Up
None,	Down,	Page_Down
Shift_L,	Up,	Control_L|N
Shift_L,	Down,	Control_L|P
Control_L,	Up,	Control_L|N
Control_L,	Down,	Control_L|P

"^vim - "
None,	Up,	Page_Up
None,	Down,	Page_Down

"^Xdvi:"
None,	Up,	Page_Up
None,	Down,	Page_Down

"^xv "
None,	Up,	Backspace
None,	Down,	Space

"^XVcontrols"
None,	Up,	Backspace
None,	Down,	Space

".*"
@Priority=-1000
@Exclude
@Repeat

