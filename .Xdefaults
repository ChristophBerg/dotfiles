! $Id$
*background:		white
*foreground:		black
*Scrollbar*foreground:	LightSkyBlue
*Scrollbar*thickness:	8
*Scrollbar*thumb:	None
XClock*analog:		false
XClock*background:	LightSkyBlue
XClock*padding:		0
XClock*update:		1
XClock*font:		-misc-fixed-medium-*-*-*-12-*-*-*-*-*-*-*
XDvi.expert:		true
XDvi.highlight:		blue
XDvi.keepPosition:	true
XEmacs.font:		-*-Fixed-medium-r-*-*-*-160-*-*-*-*-ISO8859-1
Fig*inches:		false
Fig*library_dir:	~/lib/xfig
Fig*PaperSize:		A4
Fig*specialtext:	true
Netscape*blinkingEnabled: false
Netscape*installColormap: true
XLock.logoutButton:	-1
XLock.background:	black
XLock.foreground:	red
XLock.echokeys:		true
XTerm*color3:		orange
XTerm*color5:		maroon
XTerm*color11:		orange
XTerm*color13:		maroon
XTerm*cursorColor:	brown
XTerm*font:		fixed
XTerm*highlightColor:	LimeGreen
!XTerm*highlightSelection: true
XTerm*scrollBar:	true
XTerm*scrollKey:	true
XTerm*scrollTtyOutput:	false
XTerm*saveLines:	500
XTerm*utmpInhibit:	true
XTerm*visualBell:	true
xterm*VT100.Translations: #override <Key>BackSpace: string(0x08) \n\
	Shift <KeyPress> Prior:scroll-back(2,halfpage) \n\
	Shift <KeyPress> Next:scroll-forw(2,halfpage) \n\
	<Btn4Down>:scroll-back(2,halfpage,m)     \n\
	<Btn5Down>:scroll-forw(2,halfpage,m)
!\n\
!	<Key>Delete:	string(0x1b) string("[3~")\n\
!	<Key>Home:	string(0x1b) string("[1~")\n\
!	<Key>End:	string(0x1b) string("[4~")\n\
!	Ctrl<Key>Prior:	string(0x1b) string("[40~")\n\
!	Ctrl<Key>Next:	string(0x1b) string("[41~")
xv.preset1.igraf: S 3 : 0,0 : 127,137 : 255,255
xv.preset2.igraf: S 3 : 0,0 : 127,147 : 255,255
xv.preset3.igraf: S 3 : 0,0 : 127,157 : 255,255
xv.preset4.igraf: S 3 : 0,0 : 127,167 : 255,255
xv.*.rgraf: S 4 : 0,0 : 64,64 : 192,192 : 255,255
xv.*.ggraf: S 4 : 0,0 : 64,64 : 192,192 : 255,255
xv.*.bgraf: S 4 : 0,0 : 64,64 : 192,192 : 255,255

pvs.activedisplayforeground: red
pvs.font:		9x15
pvs.geometry:		87x60+0+0
