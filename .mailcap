application/pdf; acroread '%s'; test=which acroread > /dev/null
application/postscript; gv '%s'
application/x-gunzip; gunzip -c '%s'; copiousoutput; description=Gzipped data
application/x-perl; xterm -e less %s
audio/x-pn-realaudio; realplay '%s'
image/*; xv '%s'; test=test -n "$DISPLAY" && which xv > /dev/null
text/html; /usr/bin/w3m '%s';		needsterminal; description=HTML Text; nametemplate=%s.html; test=test -x /usr/bin/w3m
text/html; /usr/bin/w3m -dump '%s';	copiousoutput; description=HTML Text; nametemplate=%s.html; test=test -x /usr/bin/w3m
text/html; /usr/bin/elinks '%s';	needsterminal; description=HTML Text; nametemplate=%s.html; test=test -x /usr/bin/elinks
text/html; /usr/bin/elinks -dump '%s';	copiousoutput; description=HTML Text; nametemplate=%s.html; test=test -x /usr/bin/elinks
text/html; /usr/bin/links '%s';		needsterminal; description=HTML Text; nametemplate=%s.html; test=test -x /usr/bin/links
text/html; /usr/bin/links -dump '%s';	copiousoutput; description=HTML Text; nametemplate=%s.html; test=test -x /usr/bin/links
