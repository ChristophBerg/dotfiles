#!/bin/sh
# $Id$

case "`uname -srm`" in
	Linux*86)	echo "linux-intel" ;;
	Linux*alpha)	echo "linux-alpha" ;;
	Linux*sparc)	echo "linux-sparc" ;;
	SunOS*4.*sun*)	echo "sunos4-sparc" ;;
	SunOS*5.*sun*)	echo "solaris-sparc" ;;
	SunOS*5.*86*)	echo "solaris-intel" ;;
	FreeBSD*86)	echo "freebsd-intel" ;;
	NetBSD*sparc)	echo "netbsd-sparc" ;;
	IRIX*5*IP*)	echo "irix-sgi" ;;
	*)		echo "unknown" 
			echo "os: warning: os type unknown" 1>&2 ;;
esac
