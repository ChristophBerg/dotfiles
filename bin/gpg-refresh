#!/bin/sh

SERVERS=$(host subkeys.pgp.net | cut -d ' ' -f 4 | \
	egrep -v '64.71.173.107|114.31.78.196|213.239.212.133')

for server in $SERVERS ; do
	getent hosts $server
	gpg --keyserver $server --recv $PGPKEY $PGPKEY2 $PGPKEY3
	echo
done

for server in $SERVERS ; do
	getent hosts $server
	gpg --keyserver $server --send $PGPKEY $PGPKEY2 $PGPKEY3
	echo
done
