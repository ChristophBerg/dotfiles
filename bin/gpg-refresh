#!/bin/sh

SERVERS=$(host subkeys.pgp.net | cut -d ' ' -f 4 | egrep -v '221.133.213.196|114.31.78.196|208.72.157.55|64.71.173.107')

for server in $SERVERS ; do
	host $server
	gpg --keyserver $server --recv $PGPKEY $PGPKEY2
done

for server in $SERVERS ; do
	host $server
	gpg --keyserver $server --send $PGPKEY $PGPKEY2
done
